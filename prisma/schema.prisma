generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Login{
  username String  @unique
  password String
}


model Signup{
  id String @id @default(cuid())
  username String @unique
  email String
  phone String
  password String 
  Income Int
  expenditure Expenditure[]
}

model Expenditure{
   id String @id @default(cuid())
  expense Int
  name String  @default("")
  date DateTime
  userId String
  signup Signup @relation(fields:[userId],references:[id])
}
